add std;
add math;

std.print("Enter n: ");
set n: int = std.input() as int;

set mut i: int = 2;
set mut is_prime: bool = true;

during i as float < math.sqrt(n) {
    when n % i == 0 {
        is_prime = false;
        std.println("Exit from i = " + i as str);
        br;
    }

    i = i + 1;
}

when is_prime {
    std.println(n as str + " is a prime number");
} alt {
    std.println(n as str + " is not a prime number");
}
